<%- include ('partials/head',{title:'Inicio'} )%>

<body class="container">  
  <%- include("partials/header") %>

  <div class="col xs-8 col-xs-offset-2">
    <h2 class="text-center">Write a new entry</h2>
    <form method="post" role="form">
      <div class="form-group">
        <label for="title">Title</label>
        <input name="title" id="title" type="text" class="form-control" placeholder="Entry a New Title" required></input>
      </div>
      <div>
        <label for="body">Entry text</label>
        <textarea class="form-control" name="body" id="body" rows="3" required></textarea>
      </div>
      <div class="form-group">
        <input type="submit" value="post-entry" class="btn btn-primary mt-2">
      </div>
    </form>
  </div>
</body>

</html>
