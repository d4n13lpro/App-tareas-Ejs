<%- include ('partials/head',{title:'Inicio'} )%>

<body class="container">    
    <%- include ('partials/header' )%>
    <% if (entries.length > 0) { %>
        <table class="table">
            <thead>
                <tr>
                    <th>Published</th>
                    <th>Title</th>
                    <th>Content</th>
                </tr>
            </thead>
            <tbody>
                <% entries.forEach(function(entry){ %>
                    <tr>
                        <td><%= entry.published %></td>
                        <td><%= entry.title %></td>
                        <td><%= entry.content %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    <% } else { %>
        <p>No existen entradas</p>
    <% } %>
</body>
</html>
